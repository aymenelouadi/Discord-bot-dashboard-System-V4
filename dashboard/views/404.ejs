  <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/6.4.0/css/all.min.css">
  <style>
    .logo,.main-header,body{display:flex}.content-header h1,.logo{font-weight:600;color:var(--discord-text)}.content-header h1,.logo,body{color:var(--discord-text)}.form-control:focus,.main-header{background-color:var(--discord-dark)}.logo,.nav-link{text-decoration:none}:root{--discord-dark:#36393f;--discord-darker:#2f3136;--discord-darkest:#202225;--discord-border:#424549;--discord-text:#ffffff;--discord-text-muted:#b9bbbe;--discord-blurple:#5865f2;--discord-blurple-hover:#4752c4;--discord-yellow:#faa81a;--discord-yellow-hover:#e09515;--discord-green:#3ba55c}*{margin:0;padding:0;box-sizing:border-box;font-family:'Segoe UI',Tahoma,Geneva,Verdana,sans-serif}body{background-color:var(--discord-darker);min-height:100vh;flex-direction:column}.main-header{height:60px;align-items:center;justify-content:space-between;padding:0 20px;border-bottom:2px solid var(--discord-border)}.logo{align-items:center;font-size:18px;transition:color .3s}.btn,.nav-link{transition:background-color .3s;display:flex}.logo:hover{color:var(--discord-blurple)}.logo-img{width:32px;height:32px;margin-right:10px;border-radius:8px;object-fit:cover}.content-wrapper{flex:1;display:flex;flex-direction:column;padding:20px}.content-header{margin-bottom:30px;padding-bottom:15px;border-bottom:1px solid var(--discord-border)}.content-header h1{font-size:24px}.error-page{display:flex;flex-direction:column;align-items:center;justify-content:center;text-align:center;padding:40px 20px;max-width:800px;margin:0 auto}.headline{font-size:120px;font-weight:700;line-height:1;margin-bottom:20px;color:var(--discord-yellow);text-shadow:0 4px 8px rgba(0,0,0,.3)}.error-content{max-width:600px}.error-content h3{font-size:24px;margin-bottom:15px;color:var(--discord-text);display:flex;align-items:center;justify-content:center;gap:10px}.error-content p{font-size:16px;line-height:1.6;margin-bottom:25px;color:var(--discord-text-muted)}.search-form{max-width:500px;margin:0 auto}.input-group{display:flex;border-radius:4px;overflow:hidden;box-shadow:0 2px 5px rgba(0,0,0,.2)}.form-control{flex:1;padding:12px 15px;border:none;background-color:var(--discord-darkest);color:var(--discord-text);font-size:16px}.form-control::placeholder{color:var(--discord-text-muted)}.form-control:focus{outline:0}.input-group-btn{display:flex}.btn{padding:12px 20px;border:none;background-color:var(--discord-blurple);color:#fff;font-weight:600;cursor:pointer;align-items:center;justify-content:center}.btn:hover{background-color:var(--discord-blurple-hover)}.btn-warning{background-color:var(--discord-yellow)}.btn-warning:hover{background-color:var(--discord-yellow-hover)}.navigation-links{margin-top:30px;display:flex;gap:15px;justify-content:center;flex-wrap:wrap}.nav-link{padding:10px 20px;background-color:var(--discord-dark);color:var(--discord-text);border-radius:4px;align-items:center;gap:8px}.nav-link:hover{background-color:var(--discord-blurple);color:#fff}@media (max-width:768px){.headline{font-size:80px}.error-content h3{font-size:20px;flex-direction:column}.navigation-links{flex-direction:column;align-items:center}.nav-link{width:200px;justify-content:center}}
